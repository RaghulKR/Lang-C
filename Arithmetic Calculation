#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int num1, num2;

    scanf("%d", &num1);
    scanf("%d", &num2);

    printf("Addition=%d\n", num1 + num2);
    printf("Subtraction=%d\n", num1 - num2);
    printf("Multiplication=%d\n", num1 * num2);
    printf("Division=%d\n", num1 / num2);
    printf("Modulo=%d\n", num1 % num2);

    return 0;
}
