#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float num1;
    int num2, base, power;

    scanf("%f", &num1);
    scanf("%d", &num2);
    scanf("%d", &base);
    scanf("%d", &power);

    printf("%d\n", (int)floor(num1));
    printf("%d\n", (int)ceil(num1));
    printf("%d\n", (int)sqrt(num2));
    printf("%d\n", (int)pow(base, power));

    return 0;
}
